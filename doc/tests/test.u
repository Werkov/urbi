//                                      -*- urbi -*-

// Too many unpredictable changes in the error messages.
LookupError.fixSpellingErrors = false |

var obj = Object.new |
do (obj)
{
  function met_hod() {};
  function 'if'() {};
  var '[]';
  function msg {};
} |

var target = Object.new |
var target.body |
var initialization = Object.new |
var condition = false |
var increment = Object.new |
var collection = [] |
var otherwise = Object.new |

var value = 0 |
var value_one = 1 |
var action_one = 1 |
var value_two = 2 |
var action_two = 2 |
var default_action = 3 |
var Global.mytag = Tag.new |

// We want random to become deterministic.
do (Float)
{
  var this.random_counter = 0;
  // chosen by fair dice roll.
  // guaranteed to be random.
  var this.random_values = [3, 1, 2];
  removeSlot("random");
  var random =
    function ()
    {
      var res = random_values[random_counter];
      // We don't want COW here, as the doc calls "5.random" repeatedly.
      Float.random_counter = (random_counter + 1) % random_values.size;
      res
    }
} | {};
