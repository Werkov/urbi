class Object.Group
{
  function init ()
  {
    var self.members = [];
  };

  function add(g)
  {
    members.push_back(g);
  };

  // Implements addgroup.  Cannot use "group" as an identifier,
  // reserved by k1.
  function addGroups(g)
  {
    members += g;
  };

  function remove(g)
  {
    members = members.remove(g);
  };

  // Implements delgroup.  Cannot use "group" as an identifier,
  // reserved by k1.
  function removeGroups(g)
  {
    members = self.members - g;
  };

  void;
};
