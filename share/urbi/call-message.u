/*---------------.
| Call message.  |
`---------------*/

class Global.CallMessage
{
  function argString(i)
  {
    args[i].asString
  };

  function evalArgAt(i)
  {
    args[i].eval
  };

  function argAt(i)
  {
    args[i].value
  };

  function evalArgs()
  {
    args.map(function (arg) { arg.eval })
  };

  function argsCount
  {
    args.size
  };
};
