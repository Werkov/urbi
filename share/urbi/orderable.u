/*------------.
| Orderable.  |
`------------*/

class Global.Orderable
{
  // Each operator points to the following one, so that defining one
  // suffices.
  function '<'  (rhs) { this <= rhs  && this != rhs };
  function '<=' (rhs) { rhs  >  this || this == rhs };
  function '>'  (rhs) { this >= rhs  && this != rhs };
  function '>=' (rhs) { rhs  <  this || this == rhs };
};
